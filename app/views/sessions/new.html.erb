<!-- <h1>Log in</h1> -->
 <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
<center>

  <div class="container">
    <div class="connexion">
      <div class="label_mail">
        <%= f.text_field :title, placeholder: "Entrer votre votre email" %>
        <%= f.password_field :title, placeholder: "Entrer votre password" %>

        <input type="text" placeholder="Entrer votre Email">
      </div>
      <div class="label_pass">
          <input type="text" placeholder="password_field :password">
          <%= f.password_field :password %>
      </div>
      <div class="">
        <%= f.submit "Connexion" %> <br>
        <%= f.submit "Sign up" %>
      </div>

    </div>

  </div>
</center>

<center>
<table cellpadding=2 cellspacing=2 width ="50%"  height="50px" border="3px">
   <tr>
     <td class="label_connexion"> <%= f.label :email %></td>
     <td> <%= f.email_field :email %></td>
   </tr>
   <tr>
     <td><%= f.label :password %></td>
     <td><%= f.password_field :password %></td>
   </tr>
 </table><br>
 <table>
   <tr>
   <td colspan="2"> <%= f.submit "Connexion" %></td>
   <td colspan="2"> <%= f.submit "Sign up" %></td>
   </tr>
 </table>
 </center>
 <% end %>
